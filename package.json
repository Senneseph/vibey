{
    "name": "vibey",
    "displayName": "Vibey",
    "description": "Offline-first AI Coding Agent",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vibey.start",
                "title": "Vibey: Start Agent"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vibey-sidebar",
                    "title": "Vibey",
                    "icon": "$(robot)"
                }
            ]
        },
        "views": {
            "vibey-sidebar": [
                {
                    "type": "webview",
                    "id": "vibey.chatView",
                    "name": "Chat"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.90.0",
        "@types/glob": "^8.1.0",
        "@types/node": "20.x",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "zod": "^3.23.8"
    }
}